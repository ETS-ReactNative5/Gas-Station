[alias]
  c = commit
  ca = commit -a
  cm = commit -m
  cam = commit -am
  d = diff
  dc = diff --cached
  l = log --graph --pretty=format:"%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset" --abbrev-commit
  master = !git commit -am 'Quick commit.' && git push origin master
[color]
  branch = auto
  diff = auto
  interactive = auto
  status = auto
[core]
  editor = vi
  excludesfile = ~/.gitignore
  autocrlf = input
[difftool]
  prompt = false
[merge]
  tool = kdiff3
[push]
  default = simple
[url "https://"]
  insteadOf = git://
[user]
  name = Brian Zalewski
  email = brian@megabyte.space
[web]
  browser = google-chrome
